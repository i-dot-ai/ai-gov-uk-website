---
title: Site Map
templateEngineOverride: njk
permalink: "site-map.js"
---

module.exports = [
  '/',
  '/about',
  '/join',
  '/projects',
  {% for project in projects %}
    '{{ project.url }}',
  {% endfor %}
  '/blogs',
  {% for blog in blogs %}
    {% if blog.summaryShort %} {# This ensures the CMS preview blog isn't displayed here #}
      '/blogs/{{ blog.title | slugify  }}',
    {% endif %}
  {% endfor %}
  '/accessibility'
];