{% macro infographic () %}

  <style>
      .iai-infographic {
          background: var(--Linear, linear-gradient(157deg, #F6EEF0 -3.69%, #F8F8F8 139.85%));
          display: flex;
          flex-direction: column;
          font-size: 1rem;
          gap: 1rem;
          grid-template-columns: repeat(10, 1fr);
          grid-template-rows: repeat(3, auto);
          line-height: 1.1;
          margin-top: 2rem;
          max-width: 68rem;
          padding: 1rem;
          position: relative;
      }
      .iai-infographic__card {
          background-color: white;
          border-radius: 0.25rem;
          box-shadow: -4px -4px 10px 0px rgba(0, 0, 0, 0.15);
          overflow: hidden;
          padding: 0.875rem;
          position: relative;
          z-index: 1;
      }
      .iai-infographic__card--delivery-partners {
          align-items: center;
          display: grid;
          font-size: 1.125rem;
          gap: 1rem;
          grid-column-start: 4;
          grid-column-end: 11;
          grid-row: 1;
          grid-template-columns: 1fr;
      }
      .iai-infographic__card--website {
          font-size: 1rem;
          grid-column-start: 1;
          grid-column-end: 3;
          grid-row-start: 2;
          grid-row-end: 4;
          line-height: 1.3;
      }
      .iai-infographic img {
          margin-top: 0;
      }
      .iai-infographic__card--delivery-partners img {
          margin-left: 0.5rem;
          width: 100%;
      }
      .iai-infographic__inner-box {
          background-color: white;
          border: 1px solid #aaa;
          border-radius: 0.25rem;
          margin-top: 0.5rem;
          padding: 0.25rem 0.5rem;
          position: relative;
      }
      .iai-infographic__card--products {
          grid-row-start: 2;
          grid-row-end: 4;
      }
      .iai-infographic__card--tech-partners {
          font-size: 1.25rem;
          grid-column-start: 3;
          grid-column-end: 5;
      }
      .iai-infographic__card--tech-partners img {
          display: block;
          margin-bottom: -1rem;
          position: relative; top: -0.75rem;
      }
      .iai-infographic__bold {
          font-weight: bold;
      }
      .iai-infographic__flex--mobile {
          align-items: center;
          display: flex;
          gap: 0.5rem;
      }
      .iai-infographic__pink {
          color: var(--iai-pink);
          font-weight: bold;
      }
      .iai-infographic__teal {
          color: var(--iai-teal);
          font-weight: bold;
      }
      @media (min-width: 1024px) {
          .iai-infographic {
              display: grid;
          }
          .iai-infographic::after {
              background-color: var(--iai-pink);
              content: "";
              height: 100%;
              position: absolute;
              right: 15%;
              top: 0;
              transform: skewX(-15deg);
              width: 17%;
          }
          .iai-infographic__card--delivery-partners {
              gap: 0.125rem;
              grid-template-columns: 1fr 3fr;
          }
          .iai-infographic__card--tech-partners img {
              margin-bottom: -3rem;
              position: relative; top: -2.5rem;
          }
          .iai-infographic__flex {
              align-items: center;
              display: flex;
              gap: 0.5rem;
          }
      }
  </style>
  <section class="iai-infographic">
      <div style="grid-column-start: 1; grid-column-end: 4; max-width: 16rem; position: relative; z-index: 10;">
          <span style="font-size: 1.5rem;">In the first</span>
          <span class="iai-infographic__flex--mobile" style="align-items: end;">
              <span class="iai-infographic__bold" style="font-size: 5.5rem;">12</span>
              <span style="position: relative; top: -0.875rem;">
                  <span class="iai-infographic__bold" style="font-size: 2rem;"> months</span>
                  <span style="display: block; font-size: 1.25rem;">as a team</span>
              </span>
          </span>
          <span class="iai-infographic__flex--mobile" style="align-items: end; justify-content: flex-end; margin-top: 0.125rem;">
              <img width="56" src="/img/i-dot-ai-Official-Logo.svg" alt=""/>
              <span style="font-size: 0.625rem; line-height: 1.3; text-align: center; width: 5rem;">Incubator for Artificial Intelligence</span>
              <span style="font-size: 1rem;">have:</span>
          </span>
      </div>
      <div class="iai-infographic__card iai-infographic__card--delivery-partners">
          <div style="line-height: 1.2;">
              <span>Worked with</span>
              <span class="iai-infographic__pink" style="display: block; font-size: 1.375rem; margin: 0.25rem 0;">delivery partners</span>
              <span>across</span>
              <span class="iai-infographic__bold">public sector agencies <br/>&amp; departments</span>
          </div>
          <img src="/img/delivery-partners.svg" alt="10 Downing Street. Ministry of Justice. Cabinet Office. Department of Health and Social Care. Department for Science, Innovation, and Technology. Home Office. Department for Education. National Cyber Security Centre. Department for Energy Security and Net Zero. Infrastructure and Projects Authority. Oak National Academy. Citizens Advice. National Energy System Operator. OFGEM. NHS England."/>
      </div>
      <div class="iai-infographic__card iai-infographic__card--website">
          <div style="max-width: 20rem; position: relative; z-index: 10;">
              <span class="iai-infographic__bold" style="font-size: 1.25rem;">Over</span>
              <span class="iai-infographic__pink" style="font-size: 2.5rem; line-height: 0.8;">115k</span>
              <span class="iai-infographic__bold" style="font-size: 1.25rem;">website views</span>
              <span>from over</span>
              <span class="iai-infographic__bold">150</span>
              <span>countries globally</span>
          </div>
          <img style="margin-bottom: -0.25rem; transform: scale(1.4);" src="/img/infographic-map.png" alt=""/>
          <div class="iai-infographic__inner-box" style="display: inline-block;">
              <span style="display: block; font-size: 0.625rem;">Total Visits</span>
              <span class="iai-infographic__teal" style="font-size: 1rem;">54.8k</span>
          </div>
          <br/>
          <div class="iai-infographic__inner-box" style="display: inline-block;">
              <span style="display: block; font-size: 0.625rem;">Total page views</span>
              <span class="iai-infographic__pink" style="font-size: 1rem;">117k</span>
          </div>
      </div>
      <div class="iai-infographic__card iai-infographic__card--products" style="grid-column-start: 3; grid-column-end: 6;">
          <div class="iai-infographic__flex" style="gap: 0.75rem;">
              <span class="iai-infographic__pink" style="font-size: 3rem;">11</span>
              <span class="iai-infographic__bold" style="font-size: 1.25rem;">products in <br/>Alpha or Beta</span>
          </div>
          <span class="iai-infographic__teal">22 prototypes</span>
          <span style="display: block; line-height: 1.5;">from a pipeline of <span class="iai-infographic__bold">57</span> projects</span>
          <div class="iai-infographic__flex--mobile" style="align-items: flex-start; gap: 1.25rem; justify-content: center; margin-top: 0.5rem;">
              <p class="iai-infographic__flex--mobile" style="flex-direction: column; gap: 0.25rem; margin: 0.25rem 0; width: 7rem;">
                  <img width="64" src="/img/project-icon-redbox.svg" alt="Redbox"/>
                  <span style="font-size: 0.625rem; line-height: 1.3; margin-top: 0; text-align: center;">1000+ users</span>
              </p>
              <p class="iai-infographic__flex--mobile" style="flex-direction: column; gap: 0.25rem; margin: 0.25rem 0; width: 7rem;">
                  <img width="64" src="/img/project-icon-lex.svg" alt="Lex"/>
                  <span style="font-size: 0.625rem; line-height: 1.3; margin-top: 0; text-align: center;">Trialled in Ministerial private offices</span>
              </p>
          </div>
          <div class="iai-infographic__flex--mobile" style="align-items: flex-start; gap: 1.25rem; justify-content: center; margin-top: 0.125rem;">
              <p class="iai-infographic__flex--mobile" style="flex-direction: column; gap: 0.25rem; margin: 0.25rem 0; width: 7rem;">
                  <img width="64" src="/img/project-icon-consult.svg" alt="Consult"/>
                  <span style="font-size: 0.625rem; line-height: 1.3; margin-top: 0; text-align: center;">400x cheaper and 1000x times faster than human analysts</span>
              </p>
              <p class="iai-infographic__flex--mobile" style="flex-direction: column; gap: 0.25rem; margin: 0.25rem 0; width: 7rem;">
                  <img width="64" src="/img/project-icon-caddy.svg" alt="Caddy"/>
                  <span style="font-size: 0.625rem; line-height: 1.3; margin-top: 0; text-align: center;">9000+ interactions per month in 5 Citizens Advice offices</span>
              </p>
          </div>
      </div>
      <div class="iai-infographic__card" style="grid-column-start: 6; grid-column-end: 8;">
          <div class="iai-infographic__flex--mobile">
              <img width="72" src="/img/infographic-hires.svg" alt=""/>
              <div style="line-height: 1; margin-left: 0.75rem;">
                  <span class="iai-infographic__pink" style="font-size: 2.75rem;">51</span>
                  <span class="iai-infographic__bold" style="font-size: 1.125rem;">hires</span>
                  <span style="display: block; font-size: 0.75rem; margin-top: 0.125rem;">so far...</span>
              </div>
          </div>
          <div class="iai-infographic__flex--mobile" style="border-top: 1px solid var(--iai-teal); margin-bottom: -1rem; margin-top: 0.75rem; padding-top: 0.25rem;">
              <svg width="40" height="40" viewBox="0 0 50 50" fill="none" aria-hidden="true" focusable="false">
                  <path d="M45.1477 14.6807L27.7935 32.0348L17.5852 21.8265L4.31433 35.0973" stroke="#07A65C" stroke-width="3" stroke-linecap="round" stroke-linejoin="round"/>
                  <path d="M32.8977 14.6807H45.1477V26.9307" stroke="#07A65C" stroke-width="3" stroke-linecap="round" stroke-linejoin="round"/>
              </svg>
              <p class="iai-infographic__teal" style="font-size: 0.75rem; margin: 0;">17 external companies</p>
          </div>
      </div>
      <div class="iai-infographic__card iai-infographic__card--code" style="grid-column-start: 8; grid-column-end: 11;">
          <span class="iai-infographic__bold" style="display: block; font-size: 1.25rem;">Deployed new code</span>
          <span class="iai-infographic__pink"><span style="display: inline-block; font-size: 2.5rem; margin-top: 0.25rem;">1836</span> times <span class="iai-infographic__teal" style="display: inline-block; font-weight: 400; margin-left: 0.25rem;" aria-hidden="true"> &lt;/&gt;</span></span>
          <p class="iai-infographic__flex--mobile" style="border-top: 1px solid var(--iai-teal); margin-bottom: -0.25rem; margin-top: 0.75rem; padding-top: 0.75rem;">
              <img width="28" src="/img/infographic-github.svg" alt=""/>
              <span class="iai-infographic__teal" style="font-size: 2rem;">17</span>
              <span style="font-size: 0.75rem; line-height: 1.3;">Open source <br/>Github repositories</span>
          </p>
      </div>
      <div class="iai-infographic__card iai-infographic__card--tech-partners" style="grid-column-start: 6; grid-column-end: 11;">
          <p class="iai-infographic__pink" style="margin: 0;">Working with partners</p>
          <span style="font-size: 1rem;">across tech and research sectors</span>
          <img src="/img/infographic-tech-partners.svg" alt="AWS. The Alan Turing Institute. Microsoft. Cohere. Open AI. Anthropic. Google. Elastic. Imperial. Nesta. Vector Institute."/>
      </div>
  </section>

{% endmacro %}